{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://swapi.co/schemas/films.json",
  "description": "All films scheme",
  "type": "object",
  "properties": {
    "count": {
      "type": "integer"
    },
    "next": {
      "type": "null"
    },
    "previous": {
      "type": "null"
    },
    "results": {
      "type": "array",
      "items": {
        "$ref": "/film.json"
      }
    }
  },
  "required": [ "count", "next", "previous", "results" ]
}
